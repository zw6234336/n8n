# n8n MySQL 数据库配置示例

# 数据库类型
DB_TYPE=mysqldb

# MySQL 数据库配置
DB_MYSQLDB_HOST=localhost
DB_MYSQLDB_PORT=3306
DB_MYSQLDB_DATABASE=n8n
DB_MYSQLDB_USER=root
DB_MYSQLDB_PASSWORD=zhangwei

# 可选配置
DB_TABLE_PREFIX=""
DB_PING_INTERVAL_SECONDS=2

# n8n 其他配置
N8N_BASIC_AUTH_ACTIVE=false
N8N_SECURE_COOKIE=false
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# n8n 运行环境配置
NODE_ENV=production

# 启动命令配置
# 正确的启动命令：cd packages/cli/bin && nohup ./n8n start > ../../../n8n.log 2>&1 &
# 启动脚本使用方式：
# 1. 确保在 n8n 项目根目录
# 2. 运行命令：./start-offline.sh
# 3. 或手动启动：cd packages/cli/bin && ./n8n start

# Nacos 配置管理（可选）
NACOS_SERVER=http://192.168.1.210:30002
NACOS_NAMESPACE=""
NACOS_GROUP=DEFAULT_GROUP
NACOS_DATA_ID=n8n-pro.yml
NACOS_USERNAME=nacos
NACOS_PASSWORD=nacos
NACOS_CONFIG_FORMAT=yaml

# 加密密钥（系统自动生成，如已存在请勿修改）
# N8N_ENCRYPTION_KEY=your_encryption_key_here

# 中文汉化配置
N8N_DEFAULT_LOCALE=zh-CN

# 时区设置
TZ=Asia/Shanghai
